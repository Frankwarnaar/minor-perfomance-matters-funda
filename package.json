{
  "name": "funda",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/app.js",
  "dependencies": {
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-ejs-extend": "0.0.1",
    "request": "^2.81.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "less": "^2.7.2",
    "less-monitor": "^0.2.0",
    "less-watch-compiler": "^1.4.2",
    "ngrok": "^2.2.6",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "start": "node src/server/app.js",
    "develop": "nodemon src/server/app.js",
    "expose": "ngrok http 3000",
    "js": "browserify src/client/js/app.js > build/client/js/bundle.js -t [ babelify --presets [es2015] ]",
    "watchify": "watchify src/client/js/app.js -o build/client/js/bundle.js -t [ babelify --presets [es2015] ]",
    "less": "lessc src/client/styles/app.less build/client/css/app.css",
    "watch-less": "less-watch-compiler ./src/client/styles build/client/css app.less",
    "critical-css": "critical src/server/views/index.ejs --base build/client/css > build/client/css/app.css"
  },
