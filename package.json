{
  "name": "funda",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/app.js",
  "dependencies": {
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-ejs-extend": "0.0.1",
    "request": "^2.81.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "less": "^2.7.2",
    "less-monitor": "^0.2.0",
    "less-watch-compiler": "^1.4.2",
    "ngrok": "^2.2.6",
    "watchify": "^3.9.0"
  },
  "scripts": {
	"start": "npm run server & npm run watch",
	"server": "nodemon --harmony_array_includes src/server/app.js",
    "production": "npm run build && node --harmony_array_includes src/server/app.js",
    "expose": "ngrok http 3000",
    "js": "browserify src/client/js/app.js > build/client/js/bundle.js -t [ babelify --presets [es2015] ]",
    "watch:js": "watchify src/client/js/app.js --debug -o build/client/js/bundle.js -t [ babelify --presets [es2015] ]",
    "less": "lessc src/client/styles/app.less build/client/css/app.css",
    "watch:less": "less-watch-compiler ./src/client/styles build/client/css app.less",
	"build": "npm run less && npm run js",
	"watch": "npm run watch:js & npm run watch:less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frankwarnaar/minor-perfomance-matters-funda.git"
  },
  "author": "Frank Warnaar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Frankwarnaar/minor-perfomance-matters-funda/issues"
  },
  "homepage": "https://github.com/Frankwarnaar/minor-perfomance-matters-funda#readme"
}
